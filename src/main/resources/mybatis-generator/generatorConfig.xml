                        password="root"/>

        <javaModelGenerator targetPackage="com.xyg.st.dto"
                            targetProject="I:\MyWork\xygfirst\src\main\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis"
                         targetProject="I:\MyWork\xygfirst\src\main\resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="mybatis.mapper"
